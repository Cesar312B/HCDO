{% extends 'base.html.twig' %}

{% block title %}Examenes{% endblock %}

{% block body %}
    <h1>Examenes</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ examene.id }}</td>
            </tr>
            <tr>
                <th>Nombre_examen</th>
                <td>{{ examene.nombreExamen }}</td>
            </tr>
            <tr>
                <th>Fecha_examen</th>
                <td>{{ examene.fechaExamen ? examene.fechaExamen|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Resultado_examen</th>
                <td>{{ examene.resultadoExamen }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ examene.Observaciones }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('examenes_index') }}">Regresar</a>

    <a href="{{ path('examenes_edit', {'id': examene.id}) }}">Editar</a>

    {{ include('examenes/_delete_form.html.twig') }}
{% endblock %}
