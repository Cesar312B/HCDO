{% extends 'base.html.twig' %}

{% block title %}Estomatogmatico index{% endblock %}

{% block body %}
    <h1>Estomatogmatico index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Opciones</th>
                <th>Observaciones</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for estomatogmatico in estomatogmaticos %}
            <tr>
                <td>{{ estomatogmatico.opciones ? estomatogmatico.opciones|json_encode : '' }}</td>
                <td>{{ estomatogmatico.observaciones }}</td>
                <td>
                    <a href="{{ path('app_estomatogmatico_show', {'id': estomatogmatico.id}) }}">show</a>
                    <a href="{{ path('app_estomatogmatico_edit', {'id': estomatogmatico.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_estomatogmatico_new') }}">Create new</a>
{% endblock %}
