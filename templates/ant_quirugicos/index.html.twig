{% extends 'base.html.twig' %}

{% block title %}AntQuirugicos index{% endblock %}

{% block body %}
    <h1>AntQuirugicos index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Cirujia</th>
                <th>Fecha</th>
                <th>Tipo_cirujia</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ant_quirugico in ant_quirugicos %}
            <tr>
                <td>{{ ant_quirugico.id }}</td>
                <td>{{ ant_quirugico.cirujia }}</td>
                <td>{{ ant_quirugico.fecha ? ant_quirugico.fecha|date('Y-m-d') : '' }}</td>
                <td>{{ ant_quirugico.tipoCirujia }}</td>
                <td>
                    <a href="{{ path('ant_quirugicos_show', {'id': ant_quirugico.id}) }}">show</a>
                    <a href="{{ path('ant_quirugicos_edit', {'id': ant_quirugico.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ant_quirugicos_new') }}">Create new</a>
{% endblock %}
